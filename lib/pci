# run "rc pci" to update
{
	sed '/^$/q' pci
	hget 'http://www.pcidatabase.com/reports.php?type=tab-delimeted'|
	tcs -f microsoft|
	sed 's/ÃƒÂ/é/g; s/´/''/g; s/Ã‚Âµ/µ/g'
} >npci && mv npci pci
exit 0

