#!/bin/rc
# run "/lib/pci" to update
rfork n
9fs boot
cd /n/boot/lib
{
	sed '/^$/q' pci
	hget 'http://www.pcidatabase.com/reports.php?type=tab-delimeted'|
	tcs -f microsoft|
	sed 's/ÃƒÂ/é/g; s/´/''/g; s/Ã‚Âµ/µ/g'
} >_pci && mv _pci pci
chmod +x pci
exit 0

