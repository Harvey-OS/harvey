</$objtype/mkfile
BIN=/$objtype/bin

TARG=rio
FILES=\
	rio\
	data\
	fsys\
	scrl\
	time\
	util\
	wctl\
	wind\
	xfid\

HFILES=dat.h\
	fns.h\

CFILES=${FILES:%=%.c}

OFILES=${FILES:%=%.$O}

UPDATE=mkfile\
	/386/bin/$TARG\
	$HFILES\
	$CFILES\

</sys/src/cmd/mkone

$O.out:	/$objtype/lib/libdraw.a /$objtype/lib/libframe.a /$objtype/lib/libthread.a /$objtype/lib/libplumb.a /$objtype/lib/libc.a
syms:V:
	vc -a $CFLAGS rio.c	>syms
	vc -aa $CFLAGS *.c 	>>syms
