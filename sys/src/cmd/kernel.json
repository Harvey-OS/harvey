{
	"KernelCmd": {
		"Cflags": [
			"-mcmodel=small",
			"-O0",
			"-fplan9-extensions",
			"-mno-red-zone",
			"-ffreestanding",
			"-fno-builtin",
			"-Wall",
			"-Wno-missing-braces",
			"-Wno-parentheses",
			"-Wno-unknown-pragmas",
			"-g"
		],
		"Libs": [
			"/$ARCH/lib/libString.a",
			"/$ARCH/lib/lib9p.a",
			"/$ARCH/lib/libndb.a",
			"/$ARCH/lib/libauth.a",
			"/$ARCH/lib/libbio.a",
			"/$ARCH/lib/libip.a",
			"/$ARCH/lib/libc.a"
		],
		"Oflags": [
			"-static",
			"-e_main"
		],
		"Post": [
			"rm *.o"
		],
		"Pre": [
			"rm -f *.o *.tag.*"
		]
	}
}
