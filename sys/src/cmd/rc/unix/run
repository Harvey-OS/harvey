cc -w -c -Iinclude pfnc.c
cc -w -c -Iinclude simple.c
cc -w -c -Iinclude subr.c
cc -w -c -Iinclude trap.c
cc -w -c -Iinclude tree.c
cc -w -c -Iinclude var.c
cc -w -c -Iinclude havefork.c
cc -w -c -Iinclude unix.c
