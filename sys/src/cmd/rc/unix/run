cc -w -c -Iinclude exec.c
cc -w -c -Iinclude getflags.c
cc -w -c -Iinclude glob.c
cc -w -c -Iinclude here.c
cc -w -c -Iinclude io.c
cc -w -c -Iinclude lex.c
