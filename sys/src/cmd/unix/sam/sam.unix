-void
-checkerrs(void)
-{
-	char buf[256];
-	int f, n, nl;
-	char *p;
-	long l;
-
-	if(statfile(errfile, 0, 0, 0, &l) > 0 && l != 0){
-		if((f=open((char *)errfile, 0)) != -1){
-			if((n=read(f, buf, sizeof buf-1)) > 0){
-				for(nl=0,p=buf; nl<3 && p<&buf[n]; p++)
-					if(*p=='\n')
-						nl++;
-				*p = 0;
-				dprint("%s", buf);
