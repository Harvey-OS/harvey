-
-	where = getenv("HOME");
-	if (where == (char *)0)
-		return;
-
-	user = getuser();
-	disp = getenv("DISPLAY");
-
-	if (disp) {
-		exname = (char *)alloc(strlen(where) + 6 + strlen(user) + 1 + strlen(disp) + 1);
-		sprint(exname, "%s/.sam.%s.%s", where, user, disp);
