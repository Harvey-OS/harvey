-	flags = fcntl(fd, F_GETFL, 0);
-	if (flags == -1 || fcntl(fd, F_SETFL, flags & ~O_NONBLOCK) == -1
-			|| open(exname, O_WRONLY) == -1) {
-		(void)close(fd);
-		removeextern();
-		return;
-	}
-
-	estart(Eextern, fd, 8192);
-	atexit(removeextern);		/* commented out on mips */
