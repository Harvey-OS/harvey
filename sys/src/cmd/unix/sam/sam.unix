-void*
-emalloc(ulong n)
-{
-	void *p;
-
-	if (n < sizeof(int))
-		n = sizeof(int);
-	p = malloc(n);
-	if(p == 0)
-		panic("malloc fails");
-	memset(p, 0, n);
-	return p;
-}
-
-void*
-erealloc(void *p, ulong n)
-{
-	p = realloc(p, n);
-	if(p == 0)
-		panic("realloc fails");
-	return p;
-}
-
-void
-exits(char *message)
-{
-
-	if (message == 0)
-		exit(0);
-	else
-		exit(1);
-}
