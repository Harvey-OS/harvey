-void
-rgrow(List *r, Posn p1, Posn n)
-{
-	Posn p;
-	int i;
-
-	if(n == 0)
-		panic("rgrow 0");
-	for(p=0,i=0; i<r->nused && p+L(i)<=p1; p+=L(i++))
-		;
-	if(i == r->nused){	/* stick on end of file */
-		if(p!=p1)
-			panic("rgrow 1");
-		if(i>0 && !T(i-1))
-			P(i-1)+=n;
-		else
-			inslist(r, i, n);
-	}else if(!T(i))		/* goes in this empty piece */
-		P(i)+=n;
-	else if(p==p1 && i>0 && !T(i-1))	/* special case; simplifies life */
-		P(i-1)+=n;
-	else if(p==p1)
