</$objtype/mkfile

TARG=8l
OFILES=\
	asm.$O\
	obj.$O\
	optab.$O\
	pass.$O\
	span.$O\
	list.$O\
	enam.$O\
	compat.$O\

HFILES=\
	l.h\
	../8c/8.out.h\

BIN=/$objtype/bin
</sys/src/cmd/mkone

# no -F because %X takes two arguments, which varargck can't represent
CFLAGS=-Vw

enam.$O:	../8c/enam.c
	$CC $CFLAGS ../8c/enam.c
