cat dgc-head.mak dvx-head.mak gs.mak jpeg.mak devs.mak dvx-tail.mak unix-end.mak >dvx-gcc.mak
cat ansihead.mak unixhead.mak gs.mak jpeg.mak devs.mak unixtail.mak unix-end.mak >unixansi.mak
cat cc-head.mak unixhead.mak gs.mak jpeg.mak devs.mak unixtail.mak unix-end.mak >unix-cc.mak
cat gcc-head.mak unixhead.mak gs.mak jpeg.mak devs.mak unixtail.mak unix-end.mak >unix-gcc.mak
