</$objtype/mkfile

LIBFILES=\
	error\
	file\
	pack\

LIB=${LIBFILES:%=%.$O}

HFILES=\
	/sys/include/venti.h\
	stdinc.h\
	error.h\
	vac.h\
	dat.h\
	fns.h\

TARG=vac unvac vacfs
BIN=/$objtype/bin

CFILES=${TARG:%=%.c} ${LIBFILES:%=%.c}

UPDATE=\
	mkfile\
	$CFILES\
	$HFILES\
	${TARG:%=/386/bin/%}

default:V: all

</sys/src/cmd/mkmany
