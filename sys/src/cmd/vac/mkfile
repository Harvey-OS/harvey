</$objtype/mkfile

LIBFILES=\
	cache\
	fs\
	file\
	source\
	error\

LIB=${LIBFILES:%=%.$O}

HFILES=\
	/sys/include/venti.h\
	stdinc.h\
	vac.h\
	dat.h\
	fns.h\

TARG=vacfs vac
BIN=/$objtype/bin

CFILES=${TARG:%=%.c} ${LIBFILES:%=%.c}

UPDATE=\
	mkfile\
	$CFILES\
	$HFILES\
	${TARG:%=/386/bin/%}

default:V: all test

test:V: $O.srcload $O.wtest $O.rtest $O.vtdump $O.vtread

</sys/src/cmd/mkmany
