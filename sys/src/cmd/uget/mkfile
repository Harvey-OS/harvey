</$objtype/mkfile

TARG=uget
CFILES= \
	cistr.c \
	crackurl.c \
	filetype.c \
	ftp.c \
	gopher.c \
	http.c \
	uget.c \
	auth.c \
	ibuf.c \
	plan9.c \

OFILES=${CFILES:%.c=%.$O} version.$O
BIN=/$objtype/bin
HFILES=mothra.h

</sys/src/cmd/mkone

version.c:	$CFILES
	date|sed 's/^... //;s/ /-/g;s/.*/char version[]="&";/' >version.c
