</$objtype/mkfile

TARG=\
	read\
	sync\
	write\

#	copy\

BIN=/$objtype/bin/venti

</sys/src/cmd/mkmany

CFLAGS=$CFLAGS -I.

extra:V: $O.devnull $O.mkroot $O.randtest $O.readlist $O.ro $O.root

all:V:		srv.all.dir
install:V:	srv.install.dir
installall:V:	srv.installall.dir
clean:V:	srv.clean.dir
nuke:V:		srv.nuke.dir

srv.%.dir:V:
	@{ cd srv && mk $stem }
