</$objtype/mkfile

LIB=/$objtype/lib/libndb.a
FILES=\
	ndbhash\
	ndbopen\
	ndbparse\
	ndbcache\
	ndbcat\
	ndbgetval\
	ndbaux\
	ndbfree\
	ipattr\
	ndbipinfo\
	ndblookval\
	csgetval\
	csipinfo\
	dnsquery\

HFILES=\
	/sys/include/ndb.h\
	ndbhf.h

CFILES=${FILES:%=%.c}

OFILES=${FILES:%=%.$O}

UPDATE=mkfile\
	/386/lib/libndb.a\
	$HFILES\
	$CFILES\

</sys/src/cmd/mksyslib

$O.out: testipinfo.$O
	$LD $prereq
