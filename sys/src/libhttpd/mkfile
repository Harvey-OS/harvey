</$objtype/mkfile

LIB=/$objtype/lib/libhttpd.a
FILES=\
	alloc\
	checkcontent\
	date\
	fail\
	gethead\
	hio\
	httpfmt\
	httpunesc\
	lower\
	okheaders\
	parse\
	parsereq\
	query\
	redirected\
	unallowed\
	urlfmt\
	urlunesc\

HFILES=\
	/sys/include/httpd.h\
	escape.h\

CFILES=${FILES:%=%.c}

OFILES=${FILES:%=%.$O}

UPDATE=mkfile\
	/386/lib/libhttpd.a\
	$HFILES\
	$CFILES\

</sys/src/cmd/mksyslib
