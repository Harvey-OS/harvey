</$objtype/mkfile

LIB=/$objtype/lib/libauthsrv.a
FILES=\
	_asgetticket\
	_asrdresp\
	authdial\
	convA2M\
	convM2A\
	convM2PR\
	convM2T\
	convM2TR\
	convPR2M\
	convT2M\
	convTR2M\
	nvcsum\
	opasstokey\
	passtokey\
	readnvram\

HFILES=\
	/sys/include/authsrv.h\

CFILES=${FILES:%=%.c}

OFILES=${FILES:%=%.$O}

UPDATE=mkfile\
	/386/lib/libauthsrv.a\
	$HFILES\
	$CFILES\

</sys/src/cmd/mksyslib
