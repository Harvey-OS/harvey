</$objtype/mkfile

LIB=/$objtype/lib/libframe.a
FILES=\
	frbox\
	frdraw\
	frdelete\
	frinit\
	frinsert\
	frptofchar\
	frselect\
	frstr\
	frutil\

HFILES=/sys/include/frame.h

CFILES=${FILES:%=%.c}

OFILES=${FILES:%=%.$O}

UPDATE=mkfile\
	/386/lib/libframe.a\
	$HFILES\
	$CFILES\

< /sys/src/cmd/mksyslib
