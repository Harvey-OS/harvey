</$objtype/mkfile

LIB=/$objtype/lib/libflate.a
FILES=\
	deflate\
	deflatezlib\
	deflateblock\
	deflatezlibblock\
	inflate\
	inflatezlib\
	inflateblock\
	inflatezlibblock\
	flateerr\
	crc\
	adler\

HFILES=\
	/sys/include/flate.h\
	zlib.h\

CFILES=${FILES:%=%.c}

OFILES=${FILES:%=%.$O}

UPDATE=mkfile\
	/386/lib/libflate.a\
	$HFILES\
	$CFILES\

</sys/src/cmd/mksyslib
