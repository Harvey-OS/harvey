	mptoui\
	mptov\
	mptouv\

ALLOFILES=${FILES:%=%.$O}
# cull things in the per-machine directories from this list
OFILES=	`{rc ./reduce $O $objtype $ALLOFILES}

HFILES=\
	/$objtype/include/u.h\
	/sys/include/mp.h\
	dat.h\

CFILES=${FILES:%=%.c}

OFILES=${FILES:%=%.$O}

UPDATE=mkfile\
	$HFILES\
	$CFILES\

</sys/src/cmd/mksyslib
