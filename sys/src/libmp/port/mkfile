</$objtype/mkfile

LIB=/$objtype/lib/libmp.a
FILES=\
	mpaux\
	mpfmt\
	strtomp\
	mptobe\
	mptole\
	betomp\
	letomp\
	mpadd\
	mpsub\
	mpcmp\
	mpmul\
	mpleft\
	mpright\
