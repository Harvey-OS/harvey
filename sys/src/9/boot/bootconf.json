{
	"bootamd64cpu": {
		"Include": [
			"/$ARCH/include/cflags.json"
		],
		"Libs": [
			"libboot.a",
			"/$ARCH/lib/libip.a",
			"/$ARCH/lib/libauth.a",
			"/$ARCH/lib/libc.a"
		],
		"Oflags": [
			"-e_main",
			"-static"
		],
		"Post": [
			"rm *.o *.a",
			"data2c ramfs_bootamd64cpu_ ../boot/bootamd64cpu.elf.out >> ../amd64/amd64cpu.c"
		],
		"Program": "bootamd64cpu.elf.out",
		"Projects": [
			"boot.json"
		],
		"SourceFiles": [
			"printstub.c",
			"bootamd64cpu.c"
		]
	}
}
