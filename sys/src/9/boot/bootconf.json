{
	"bootcpu": {
		"Include": [
			"/$ARCH/include/cflags.json"
		],
		"Libs": [
			"libboot.a",
			"/$ARCH/lib/libip.a",
			"/$ARCH/lib/libauth.a",
			"/$ARCH/lib/libc.a"
		],
		"Oflags": [
			"-e_main",
			"-static"
		],
		"Post": [
			"rm *.o *.a",
			"data2c ramfs_bootcpu_ ../boot/bootcpu.elf.out ../$ARCH/cpu.c"
		],
		"Program": "bootcpu.elf.out",
		"Projects": [
			"boot.json"
		],
		"SourceFiles": [
			"printstub.c",
			"bootcpu.c"
		]
	}
}
