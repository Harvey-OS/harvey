{
	"libboot": {
		"Cflags": [
			"-g",
			"-Wall",
			"-Werror",
			"-O0",
			"-static",
			"-ffreestanding",
			"-fno-builtin"
		],
		"Include": [
			"/sys/src/lib.json"
		],
		"Install": "/sys/src/9/boot",
		"Library": "libboot.a",
		"Pre": [
			"mksys -o bootcpu.c -mode=bootcpu.c /sys/src/sysconf.json"
		],
		"SourceFiles": [
			"aux.c",
			"bootauth.c",
			"boot.c",
			"bootcache.c",
			"bootip.c",
			"bootcpu.c",
			"bootrc.c",
			"embed.c",
			"local.c",
			"paq.c",
			"settime.c"
		]
	}
}
