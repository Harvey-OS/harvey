[
	{
		"Include": [
			"/amd64/include/cflags.json",
			"/sys/src/lib.json"
		],
		"Install": "/sys/src/9/boot",
		"Library": "libboot.a",
		"Name": "libboot",
		"Pre": [
			"mksys -o bootk8cpu.c -mode=bootk8cpu.c $HARVEY/sys/src/sysconf.json",
			"[ ! -f bootk8cpu.elf.out ] || rm *.elf.out"
		],
		"SourceFiles": [
			"aux.c",
			"bootauth.c",
			"boot.c",
			"bootcache.c",
			"bootip.c",
			"bootk8cpu.c",
			"embed.c",
			"local.c",
			"paq.c",
			"sac.c",
			"settime.c"
		]
	}
]
