{
	"core": {
		"Cflags": [
			"-O0",
			"-static",
			"-ffreestanding",
			"-fno-builtin",
			"-DKERNDATE=1433623937",
			"-g",
			"-Wall",
			"-W",
			"-Wno-sign-compare",
			"-Wno-missing-field-initializers",
			"-Wno-unused-parameter",
			"-Wno-missing-braces",
			"-Wno-parentheses",
			"-Wno-unknown-pragmas",
			"-Werror",
			"-I", "/sys/src/9/riscv",
			"-I", "/sys/src/9/port",
			"-I", "/sys/src/libc/9syscall"
		],
	    "Libs": [
			"/$ARCH/lib/klibc.a"
		],
		"Oflags": [
			"-z",
			"max-page-size=0x1000",
			"-nostdlib",
			"-g",
			"-T",
			"kernel.ld"
		],
		"Pre": [
			"mksys -o systab.c '-mode=systab.c' $HARVEY/sys/src/sysconf.json",
			"mksys -o errstr.h '-mode=errstr.h' $HARVEY/sys/src/sysconf.json"
		],
		"Projects": [
			"clean.json",
			"../boot/bootconf.json",
			"inith.json"
		],
	    "MissingSourceFiles": [
		        "acore.c",
			"archriscv.c",
			"asm.c",
		        "coreboot.c",
			"devarch.c",
			"memory.c",
			"mmu.c",
			"mp.c",
			"physalloc.c",
			"systab.c",
			"tcore.c",
			"trap.c"
		],
		"SourceFiles": [
			"asm.S",
			"arch.c",
		        "acore.c",
			"archriscv.c",
			"ctype.c",
			"devarch.c",
			"main.c",
			"map.c",
			"mmu.c",
			"qmalloc.c",
			"syscall.c",
			"systab.c",
		        "tcore.c",
			"uart.c"
		]
	}
}
