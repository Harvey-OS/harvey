{
	"inith": {
		"Cflags": [
			"-g",
			"-Wall",
			"-Werror",
			"-O0",
			"-static",
			"-mno-red-zone",
			"-ffreestanding",
			"-fno-builtin",
			"-fno-stack-protector",
			"-mcmodel=small"
		],
		"Libs": [
			"/$ARCH/lib/libc.a"
		],
		"Oflags": [
			"-e_main",
			"-static",
			"-Ttext=0x200020",
			"-z",
			"noseparate-code",
			"-z",
			"max-page-size=0x200000"
		],
		"Pre": [
			"rm -f init.h"
		],
		"Post": [
			"elf2c init.h init"
		],
		"Program": "init",
		"SourceFiles": [
			"init9.c",
			"../port/initcode.c"
		]
	}
}
