NPROC=1
</$objtype/mkfile

install:V: genall gencall
	genall

gencall:D: /sys/src/libc/9syscall/mkfile
	{
		echo '#!/bin/rc'
		sed -n -e 's/seek/_SEEK/g' -e '/switch/,/\$AS /p' $prereq
	} >$target
	chmod +x $target

nuke clean:V:
	rm -f *.[$OS] *.s gencall

installall:V:
	for(objtype in $CPUS) mk install

update:V:
	update $UPDATEFLAGS mkfile sys.h
