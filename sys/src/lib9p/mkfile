</$objtype/mkfile

LIB=/$objtype/lib/lib9p.a
FILES=\
	_post\
	dirread\
	fid\
	file\
	intmap\
	mem\
	req\
	parse\
	post\
	srv\
	tpost\
	uid\
	util\

HFILES=/sys/include/9p.h post.h

CFILES=${FILES:%=%.c}

OFILES=${FILES:%=%.$O}

UPDATE=mkfile\
	/386/lib/lib9p.a\
	/sys/src/lib9p/ramfs.c\
	$HFILES\
	$CFILES\

</sys/src/cmd/mksyslib

$O.ramfs: ramfs.$O $LIB
	$LD -o $target $prereq
