</$objtype/mkfile

LIB=/$objtype/lib/libip.a
FILES =\
	eipfmt\
	parseip\
	parseether\
	myetheraddr\
	myipaddr\
	classmask\
	bo\
	readipifc\
	ipaux\
	ptclbsum\

HFILES=\
	ip.h

CFILES=${FILES:%=%.c}

OFILES=${FILES:%=%.$O}

UPDATE=mkfile\
	/386/lib/libip.a\
	$HFILES\
	$CFILES\

</sys/src/cmd/mksyslib
