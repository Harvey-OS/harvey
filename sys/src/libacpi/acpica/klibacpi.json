{
	"KernelLibacpi": {
		"Cflags": [
			"-mcmodel=kernel",
			"-O0",
			"-mno-red-zone",
			"-ffreestanding",
			"-fno-builtin",
			"-Wall",
			"-Wno-missing-braces",
			"-Wno-parentheses",
			"-Wno-unknown-pragmas",
			"-Wuninitialized",
			"-g"
		],
		"NoCflags": [
			"-Werror"
		],
		"Projects": [
			"components/build.json"
		],
		"NoInclude": [
			"/$ARCH/include/klib.json"
		],
		"Install": "/$ARCH/lib/",
		"Library": "klibacpi.a"
		}
}
	
