</$objtype/mkfile

LIB=/$objtype/lib/libString.a

FILES=	s_alloc\
	s_append\
	s_array\
	s_copy\
	s_getline\
	s_grow\
	s_memappend\
	s_nappend\
	s_parse\
	s_read\
	s_read_line\
	s_reset\
	s_tolower\
	s_putc\
	s_terminate\
	s_unique\

HFILES=/sys/include/String.h

CFILES=${FILES:%=%.c}

OFILES=${FILES:%=%.$O}

UPDATE=mkfile\
	/386/lib/libString.a\
	$HFILES\
	$CFILES\

</sys/src/cmd/mksyslib
