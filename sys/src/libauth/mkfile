</$objtype/mkfile

LIB=/$objtype/lib/libauth.a
FILES=\
	amount\
	amount_getkey\
	attr\
	auth_attr\
	auth_challenge\
	auth_chuid\
	auth_getkey\
	auth_getuserpasswd\
	auth_proxy\
	auth_respond\
	auth_rpc\
	auth_userpasswd\
	login\
	newns\
	noworld\

HFILES=\
	/sys/include/auth.h\
	authlocal.h\

CFILES=${FILES:%=%.c}

OFILES=${FILES:%=%.$O}

UPDATE=mkfile\
	/386/lib/libauth.a\
	$HFILES\
	$CFILES\

</sys/src/cmd/mksyslib
