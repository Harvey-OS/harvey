</$objtype/mkfile

LIB=/$objtype/lib/libmach.a
FILES=executable\
	map\
	obj\
	swap\
	sym\
	access\
	machdata\
	setmach\
	v\
	k\
	q\
	0\
	2\
	6\
	8\
	x\
	9\
	5\
	7\
	vdb\
	kdb\
	qdb\
	2db\
	6db\
	8db\
	9db\
	5db\
	7db\
	vobj\
	kobj\
	2obj\
	6obj\
	8obj\
	xobj\
	9obj\
	5obj\
	7obj\
	qobj\
	vcodas\

HFILES=/sys/include/mach.h

CFILES=${FILES:%=%.c}

OFILES=${FILES:%=%.$O}

UPDATE=mkfile\
	/386/lib/libmach.a\
	$HFILES\
	$CFILES\

</sys/src/cmd/mksyslib

CFLAGS=$CFLAGS -I/sys/src/cmd
