objtype=power
</$objtype/mkfile

LIB=/$objtype/lib/libc.a
SFILES=\
	argv0.s\
	cycles.s\
	getcallerpc.s\
	getfcr.s\
	main9.s\
	main9p.s\
	memccpy.s\
	memcmp.s\
	memmove.s\
	memset.s\
	setjmp.s\
	strcmp.s\
	strncmp.s\
	tas.s\
	vlop.s

CFILES=\
	notejmp.c\
	sqrt.c\
	vlrt.c\

HFILES=/sys/include/libc.h

OFILES=${CFILES:%.c=%.$O} ${SFILES:%.s=%.$O}

UPDATE=mkfile\
	$HFILES\
	$CFILES\
	$SFILES\

</sys/src/cmd/mksyslib
