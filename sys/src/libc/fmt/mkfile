</$objtype/mkfile

LIB=/$objtype/lib/libc.a

FILES=\
	dofmt\
	dorfmt\
	errfmt\
	fltfmt\
	fmt\
	fmtfd\
	fmtlock\
	fmtprint\
	fmtquote\
	fmtrune\
	fmtstr\
	fmtvprint\
	fprint\
	print\
	runefmtstr\
	runeseprint\
	runesmprint\
	runesnprint\
	runesprint\
	runevseprint\
	runevsmprint\
	runevsnprint\
	seprint\
	smprint\
	snprint\
	sprint\
	vfprint\
	vseprint\
	vsmprint\
	vsnprint\

HFILES=/sys/include/libc.h\
	fmtdef.h\

CFILES=${FILES:%=%.c}

OFILES=${FILES:%=%.$O}

UPDATE=mkfile\
	$HFILES\
	$CFILES\

</sys/src/cmd/mksyslib

