objtype=29000
</$objtype/mkfile

LIB=/$objtype/lib/libc.a
SFILES=\
	argv0.s\
	div.s\
	main9.s\
	main9p.s\
	mul.s\
	setjmp.s\
	vlop.s\

CFILES=\
	cycles.c\
	notejmp.c\
	vlrt.c\

HFILES=/sys/include/libc.h

OFILES=${CFILES:%.c=%.$O} ${SFILES:%.s=%.$O}

UPDATE=mkfile\
	$HFILES\
	$CFILES\
	$SFILES\

</sys/src/cmd/mksyslib
