</$objtype/mkfile

LIB=/$objtype/lib/libventi.a

FILES=\
	packet\
	plan9\
	score\
	sha1-plan9\
	venti\
	errfmt\
	util\

HFILES=\
	packet.h\
	session.h\

CFILES=${FILES:%=%.c}

OFILES=${FILES:%=%.$O}

UPDATE=mkfile\
	/386/lib/libventi.a\
	$HFILES\
	$CFILES\

CFLAGS=$CFLAGS

</sys/src/cmd/mksyslib

$O.vttest: vttest.$O $OFILES
	$LD $LDFLAGS -o $target $prereq

acid:	$HFILES packet.c
	$CC $INC -a packet.c > acid || rm acid

test: $O.vttest
	$O.vttest
