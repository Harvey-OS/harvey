</$objtype/mkfile

LIB=/$objtype/lib/libventi.a

SYS=plan9

OFILES=\
	client.$O\
	debug.$O\
	errfmt.$O\
	fatal.$O\
	pack.$O\
	packet.$O\
	parsescore.$O\
	readfully.$O\
	rpc.$O\
	scorefmt.$O\
	server.$O\
	strdup.$O\
	zero.$O\
	$SYS-io.$O\
	$SYS-sha1.$O\
	$SYS-thread.$O\

HFILES=\
	packet.h\
	session.h\

UPDATE=\
	mkfile\
	$HFILES\
	${OFILES:%.$O=%.c}\
	${LIB:/$objtype/%=/386/%}\

CFLAGS=$CFLAGS

</sys/src/cmd/mksyslib

$O.vttest: vttest.$O $OFILES
	$LD $LDFLAGS -o $target $prereq

acid:	$HFILES packet.c
	$CC $INC -a packet.c > acid || rm acid

test: $O.vttest
	$O.vttest
