\def\ctab#1{\noindent$$\def\\{\cr}%
  \def\hline##1{\noalign{\nointerlineskip\vskip2pt}%
    \multispan{##1}\hrulefill\\%
    \noalign{\vskip-2pt}}%
  \tabskip0pt plus1fil
  \halign to\hsize\bgroup#1\tabskip0pt plus1fil\cr}
\def\endctab{\crcr\egroup$$}

