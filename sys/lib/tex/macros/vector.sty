\def\first@element{1}
\newcommand{\firstelement}[1]{\def\first@element{#1}}
\newcommand{\irvec}[2][n]{\ensuremath{{#2}_{\first@element},\ldots,{#2}_{#1}}}
\newcommand{\icvec}[2][n]{%
