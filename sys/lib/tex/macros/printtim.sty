\newcommand*{\PrintTime}{% 
   \bgroup% 
      \count\@cclv=\time 
      \divide  \count\@cclv by 60\relax 
      \the\count\@cclv% 
      :% 
      \multiply\count\@cclv by -60\relax 
      \advance \count\@cclv by \time 
      \ifnum\count\@cclv<10\relax 0\fi 
      \the\count\@cclv% 
   \egroup% 
   } 
\endinput
