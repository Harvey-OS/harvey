#!/bin/rc
# The destination is set to PS for the Tektronix printer, no other printer cares
generic 'aux/download -f -H/sys/lib/postscript/font -mfontmap -ptekphaser350 < $LPDEST/$FILE(1) | aux/lpdsend -d$LPDEST -H$LPMACHID -P$LPUSERID  $OUTDEV >[2] $PRINTLOG' '{cat <{echo -d$LPDEST -pnoproc -M$SCHEDLINE(1) -u$SCHEDLINE(2)} $LPDEST/$FILE(1);sleep 5} | lpsend.rc $DEST_HOST' ''
