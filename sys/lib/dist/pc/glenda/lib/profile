if(test -f '#m/mousectl') {
	echo -n accelerated > '#m/mousectl'
	echo -n 'res 3' > '#m/mousectl'
}
user=`{cat /dev/user}
home=/usr/$user
bind -a $home/bin/rc /bin
bind -a $home/bin/$cputype /bin
bind -c $home/tmp /tmp
rio -si inst/gui
echo 
echo
echo 'failed to start rio.  you can start a text-based installation by running'
echo
echo '	inst/textonly'
echo
