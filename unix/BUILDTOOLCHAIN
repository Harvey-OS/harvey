#!/bin/bash

set -x
# in case it is a file.
rm -f xbin
set -e
mkdir -p xbin

PATH=$PATH:$PLAN9/bin
export NXM=`pwd`
. $NXM/buildconfig
(cd util/8a && sh BUILD)
(cd util/8l && sh BUILD)
(cd util/8c && sh BUILD)
(cd util/6a && sh BUILD)
(cd util/6l && sh BUILD)
(cd util/6c && sh BUILD)
(cd util && sh BUILD)
(cd inferno-os && sh BUILD)
