(cd ../sys/src/cmd/6a && sh BUILD)
(cd util/6l && sh BUILD)
(cd util/6c && sh BUILD)
(cd util && sh BUILD)
(cd inferno-os && sh BUILD)

exit 0
# who cares

(cd util/8a && sh BUILD)
(cd util/8l && sh BUILD)
