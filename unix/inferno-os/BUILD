. $NXM/buildconfig
(cd lib9 && mk clean)
(cd libbio && mk clean)
(cd utils/libmach && mk clean)
(cd utils/iar && mk clean)
(cd utils/nm && mk clean)

(cd lib9 && mk all)
(cd libbio && mk all)
(cd utils/libmach && mk all)
(cd utils/iar && mk all)
(cd utils/nm && mk all)

cp utils/iar/o.out  ../xbin/ar
cp utils/nm/o.out  ../xbin/nm


