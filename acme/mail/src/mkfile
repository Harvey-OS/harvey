</$objtype/mkfile

TARG=Mail
OFILES=\
		html.$O\
		mail.$O\
		mesg.$O\
		reply.$O\
		util.$O\
		win.$O

HFILES=dat.h
LIB=

BIN=/acme/bin/$objtype

UPDATE=\
	mkfile\
	$HFILES\
	${OFILES:%.$O=%.c}\

</sys/src/cmd/mkone

$O.out: $OFILES
	$LD -o $target  $LDFLAGS $OFILES

syms:V:
	8c -a mail.c	>syms
	8c -aa mesg.c reply.c util.c win.c 	>>syms

