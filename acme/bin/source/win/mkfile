</$objtype/mkfile

TARG=win
OFILES=\
	fs.$O\
	main.$O\
	pipe.$O\
	util.$O\
	win.$O

HFILES=dat.h
LIB=/$objtype/lib/lib9p.a

BIN=/acme/bin/$objtype
</sys/src/cmd/mkone

UPDATE=\
	mkfile\
	$HFILES\
	${OFILES:%.$O=%.c}\
	${TARG:%=/acme/bin/$objtype/%}\
	
syms:V:
	8c -a main.c	>syms
	8c -aa util.c win.c 	>>syms
