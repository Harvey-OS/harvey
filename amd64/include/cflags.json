{
	"buildflags": {
		"ToolOpts": {
			"/usr/bin/clang": [
				"-mno-implicit-float"
			],
			"/usr/bin/clang-3.4": [
				"-mno-implicit-float"
			],
			"/usr/bin/clang-3.5": [
				"-mno-implicit-float"
			],
			"/usr/bin/clang-3.6": [
				"-mno-implicit-float"
			],
			"/usr/bin/clang-3.7": [
				"-mno-implicit-float"
			],
			"/opt/intel/bin/icc": [
				"-Wno-main"
			],
			"/usr/bin/gcc": [
				"-Wno-frame-address"
			],
			"/usr/bin/gcc-6": [
				"-Wno-frame-address"
			]
		},
		"Cflags": [
			"-ffreestanding",
			"-fno-builtin",
			"-fno-omit-frame-pointer",
			"-g",
			"-gdwarf-2",
			"-ggdb",
			"-mcmodel=small",
			"-mno-red-zone",
			"-O0",
			"-static",
			"-Wall",
			"-Wno-missing-braces",
			"-Wno-parentheses",
			"-Wno-unknown-pragmas"
		]
	}
}
