{
	"buildflags": {
		"Include": [
			"/$ARCH/include/$CC.json"
		],
		"Cflags": [
			"-ffreestanding",
			"-fno-builtin",
			"-fno-omit-frame-pointer",
			"-g",
			"-gdwarf-2",
			"-ggdb",
			"-mcmodel=small",
			"-mno-red-zone",
			"-O0",
			"-static",
			"-Wall",
			"-Wno-missing-braces",
			"-Wno-parentheses",
			"-Wno-unknown-pragmas"
		]
	}
}
