{
	"KernelLibs": {
		"Cflags": [
			"-mcmodel=kernel",
			"-O0",
			"-mno-red-zone",
			"-ffreestanding",
			"-fno-builtin",
			"-Wall",
			"-Wno-missing-braces",
			"-Wno-parentheses",
			"-Wno-unknown-pragmas",
			"-Wuninitialized",
			"-g"
		],
		"Oflags": [
			"-static"
		],
		"Post": [
			"rm *.o"
		],
		"Pre": [
			"rm -f *.o *.tag.*"
		]
	}
}
